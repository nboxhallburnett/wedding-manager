{
  "name": "wedding-manager",
  "version": "1.0.0",
  "description": "Wedding invitation & guest management service",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "build": "rm -rf web/public/js/*; rm -rf web/public/css/*; set -a; . .env && NODE_ENV=production npx webpack-cli build --config web/webpack.config.js",
    "build-hot": "set -a; . .env && npx webpack serve --config web/webpack.config.js --progress",
    "lint": "eslint ./ && stylelint \"./web/src/**/*.{scss,vue}\"",
    "start": "node --dns-result-order ipv4first src/index.js",
    "test": "npm run lint",
    "watch": "set -a; . .env && DEBUG=wedding-manager:* node --dns-result-order ipv4first --watch-path=./src --watch-path=./conf --watch-preserve-output src/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nboxhallburnett/wedding-manager.git"
  },
  "author": "Nathan Boxhall-Burnett <nathan@nbb.dev>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nboxhallburnett/wedding-manager/issues"
  },
  "homepage": "https://github.com/nboxhallburnett/wedding-manager#readme",
  "dependencies": {
    "compression": "1.8.1",
    "connect-mongo": "6.0.0",
    "cookie-parser": "1.4.7",
    "debug": "4.4.3",
    "ejs": "4.0.1",
    "express": "5.2.1",
    "express-rate-limit": "8.2.1",
    "express-session": "1.19.0",
    "google-auth-library": "10.5.0",
    "ical-generator": "10.0.0",
    "mongodb": "7.1.0",
    "nanoid": "5.1.6",
    "sharp": "0.34.5"
  },
  "devDependencies": {
    "@jpinsonneau/html-to-image": "1.11.13",
    "@popperjs/core": "2.11.8",
    "@stylistic/stylelint-config": "3.0.1",
    "@stylistic/stylelint-plugin": "4.0.0",
    "autoprefixer": "10.4.24",
    "bootstrap": "5.3.8",
    "css-loader": "7.1.3",
    "eslint": "9.39.2",
    "eslint-config-axway": "10.0.0",
    "eslint-plugin-vue": "10.7.0",
    "mini-css-extract-plugin": "2.10.0",
    "postcss-loader": "8.2.0",
    "purgecss-webpack-plugin": "8.0.0",
    "qr-code-styling": "1.9.2",
    "sass": "1.97.3",
    "sass-loader": "16.0.7",
    "style-loader": "4.0.0",
    "stylelint": "16.24.0",
    "stylelint-config-recommended": "17.0.0",
    "stylelint-config-recommended-vue": "1.6.1",
    "stylelint-config-standard-scss": "16.0.0",
    "vue": "3.5.27",
    "vue-loader": "17.4.2",
    "vue-router": "5.0.2",
    "vue-showdown": "4.2.0",
    "webpack": "5.105.0",
    "webpack-cli": "6.0.1",
    "webpack-dev-server": "5.2.3",
    "webpack-manifest-plugin": "6.0.1"
  },
  "engines": {
    "node": ">=24.13.0",
    "npm": ">=11.6.2"
  }
}
